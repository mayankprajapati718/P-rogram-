import java.util.Scanner;

public class Reversewords {
    public static void main(String[] args) {


    Scanner s=new Scanner(System.in);
        System.out.println("enter word");
    String k=s.next();
    String[]w=k.split("");
    for (int i= w.length-1;i>=0;i--){
        System.out.print(w[i]);
    }

}}
